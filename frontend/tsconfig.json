{
  "extends": "@tsconfig/next/tsconfig.json",
  "compilerOptions": {
    "target": "ES2016",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/public/*": ["./public/*"],
      "@/*": ["./src/*"],
      "@suilend/springsui-sdk": ["../sdk/src"],
      "@suilend/springsui-sdk/*": ["../sdk/src/*"]
    }
  },
  "exclude": ["node_modules"]
}
