{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@suilend/springsui-sdk": ["../sdk/src"],
      "@suilend/springsui-sdk/*": ["../sdk/src/*"]
    }
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
